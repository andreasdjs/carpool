extends layout

block content
  //h1= title
  #listContainer
    #productList
      h1= title
        div
          button(id='btnAdd',onClick='onAdd()')  Lägg till bil
      table
          thead
              tr
                  th P-plats
                  th Reg nummer
                  th Bilmärke
                  th Modell
                  th Storlek
                  th Bränsle

          tbody
              each item, i in vehicles.vehicles
                  tr
                      td= item.id
                      td= item.regNumber
                      td= item.brand
                      td= item.name
                      td= item.type
                      td= item.fuel
                      td
                        button(id='btnBes',onClick='onBes()')  Besiktning
                      td
                        button(id='btnDelete',onClick='onDelete()')  Radera
