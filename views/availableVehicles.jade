extends layout

block content
  //h1= title
  #listContainer
    #productList
      h1= title
      form(method='POST', action='/sent')
        input(type='hidden', name='startDate', value='#{startDate}')
        input(type='hidden', name='endDate', value='#{endDate}')
        table
          thead
            tr
              th ID
              th Reg nummer
              th Bilmärke
              th Modell
              th Storlek
              th Bränsle
              th
          tbody
            each item, i in vehicles.vehicles
              tr
                td= item.id
                td= item.regNumber
                td= item.brand
                td= item.name
                td= item.type
                td= item.fuel
                td
                  button(name='vehicleId',value='#{item.id}',id='btnAdd',type='submit') Boka